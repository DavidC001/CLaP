    "datasets_dir": "datasets",
    "contrastive":{
        "skip": false,
        "datasets": ["skiPose"],
        "mode": "multi",
        "drop_pairs": [0],
        "experiments": {
            "simsiam_18_ski":{
                "model": "simsiam",
                "base_model": "resnet18",
                "batch_size": 128,
                "learning_rate_encoder": 0.1,
                "learning_rate_head": 0.5,
                "weight_decay": 1e-4,
                "momentum": 0.9,
                "epochs": 30,
                "save_every": 100
            },
            "simclr_18_ski":{
                "model": "simclr",
                "base_model": "resnet18",
                "batch_size": 128,
                "learning_rate_encoder": 0.1,
                "learning_rate_head": 0.5,
                "weight_decay": 1e-4,
                "momentum": 0.9,
                "temperature": 0.5,
                "epochs": 30,
                "save_every": 100
            }
        }
    },
