{
    "device": "cuda",
    "models_dir": "trained_models",
    "datasets_dir": "datasets",
    "contrastive":{
        "skip": false,
        "datasets": ["skiPose"],
        "mode": "multi",
        "drop_pairs": [0],
        "experiments": {
            "moco_18_ski":{
                "model": "moco",
                "base_model": "resnet18",
                "batch_size": 128,
                "learning_rate_encoder": 0.1,   
                "learning_rate_head": 0.5,
                "momentum": 0.9,
                "weight_decay": 1e-4,
                "m": 0.999,
                "T_plus": 0.5,
                "T_negative": 0.5,
                "epochs": 20,
                "save_every": 100
            },
            "moco_50_ski":{
                "model": "moco",
                "base_model": "resnet50",
                "batch_size": 128,
                "learning_rate_encoder": 0.1,   
                "learning_rate_head": 0.5,
                "momentum": 0.9,
                "weight_decay": 1e-4,
                "m": 0.999,
                "T_plus": 0.5,
                "T_negative": 0.5,
                "epochs": 20,
                "save_every": 100
            }
        }
    },
    "pose_estimation":{
        "skip": false,
        "dataset": "skiPose",
        "batch_size": 64,
        "experiments":{
            "moco18Ski_estim_ski_5%":{
                "base_model":"resnet18",
                "model":"moco",
                "pretrained_name": "moco_18_ski",
                "architecture": [128],
                "learning_rate": 1e-3,
                "weight_decay": 1e-2,
                "epochs": 100,
                "save_every": 100,
                "LN": true,
                "activation": "gelu",
				"noise":0.5,
                "use_cluster": "./trained_models/moco_18_ski/skiPose_selected_images_20C_5%.txt"
            },
            "moco18Ski_estim_ski_random5%":{
                "base_model":"resnet18",
